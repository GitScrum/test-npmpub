{
  "name": "test-npmpub",
  "version": "0.0.12",
  "description": "test auto push/pub",
  "main": "index.js",
  "scripts": {
    "test": "echo \"test passed\"",
    "patchbeta": "np --tag=beta --no-cleanup patch",
    "patch": "np patch",
    "minor": "np minor",
    "major": "np major",
    "clean": "rm -rf lib && mkdir lib",
    "commitmsg": "conventional-changelog-lint -e",
